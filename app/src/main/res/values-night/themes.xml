<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Market_place" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="windowActionBar">false</item>

        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/wrapedWhite</item>
        <item name="colorPrimaryVariant">@color/wrapedWhite</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="MyCancelButtonTheme" parent="ThemeOverlay.AppCompat">
        <item name="colorAccent">@color/black</item>
        <item name="iconTint">@color/black</item>
    </style>
    <style name="MyAcceptButtonTheme" parent="ThemeOverlay.AppCompat">
        <item name="colorAccent">@color/wrapedWhite</item>
        <item name="iconTint">@color/wrapedWhite</item>
        <item name="backgroundColor">@color/teal_primary</item>
    </style>
</resources>